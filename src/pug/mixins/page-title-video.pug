mixin page-title-video(videoSrc, videoPoster, pageName, pageNameFull)
	section.page-section.secondary-page-title
		.secondary-page-title__inner.text-center
			h1= pageNameFull
			ul.breadcrumb
				li
					a(href='index.html') Home
				li= pageName
		video#video-background(autoplay='', loop='', muted='', plays-inline='', poster= videoPoster)
			source(src= videoSrc + `.mp4`, type='video/mp4')
			source(src= videoSrc + `.webm`, type='video/webm')
			source(src= videoSrc + `.mov`, type='video/mov')