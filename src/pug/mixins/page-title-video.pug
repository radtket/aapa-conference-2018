mixin page-title-video(videoSrc, pageName, pageNameFull)
	section.page-section.secondary-page-title
		.secondary-page-title__inner.text-center
			h1= pageNameFull
			ul.breadcrumb
				li
					a(href='index.html') Home
				li= pageName
		video#video-background(autoplay='', loop='', muted='', plays-inline='')
			source(src= videoSrc, type='video/mp4')